{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source" : "/api/**",
        "function" : "api-v1"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
